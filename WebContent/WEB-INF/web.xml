<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>SiteManger</servlet-name>
    <servlet-class>org.elluck91.first.SiteManger</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterAPI</servlet-name>
    <servlet-class>org.elluck91.first.RegisterAPI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginAPI</servlet-name>
    <servlet-class>org.elluck91.first.LoginAPI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteAPI</servlet-name>
    <servlet-class>org.elluck91.first.DeleteAPI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateAPI</servlet-name>
    <servlet-class>org.elluck91.first.UpdateAPI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InsertAPI</servlet-name>
    <servlet-class>org.elluck91.first.InsertAPI</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SiteManger</servlet-name>
    <url-pattern>/SiteManger</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterAPI</servlet-name>
    <url-pattern>/RegisterAPI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginAPI</servlet-name>
    <url-pattern>/LoginAPI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteAPI</servlet-name>
    <url-pattern>/DeleteAPI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateAPI</servlet-name>
    <url-pattern>/UpdateAPI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertAPI</servlet-name>
    <url-pattern>/InsertAPI</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>